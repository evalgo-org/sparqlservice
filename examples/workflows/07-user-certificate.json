{
  "@context": "https://schema.org",
  "@type": "SearchAction",
  "identifier": "iqs-user-certificate",
  "name": "IQS: Query User Certificate",
  "description": "Query specific user certificate from PoolParty IQS project",
  "query": {
    "@type": "SearchAction",
    "contentUrl": "get_user_certificate_iqs.sparql",
    "additionalProperty": {
      "user_id": "86774"
    }
  },
  "target": {
    "@type": "DataCatalog",
    "identifier": "IQS",
    "url": "${POOLPARTY_URL}",
    "encodingFormat": "application/rdf+xml",
    "additionalProperty": {
      "username": "${POOLPARTY_USERNAME}",
      "password": "${POOLPARTY_PASSWORD}"
    }
  }
}
