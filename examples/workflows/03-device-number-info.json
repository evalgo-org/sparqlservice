{
  "@context": "https://schema.org",
  "@type": "SearchAction",
  "identifier": "iqs-device-number-info",
  "name": "IQS: Query Device Number Info",
  "description": "Query device number information from PoolParty IQS project",
  "query": {
    "@type": "SearchAction",
    "contentUrl": "get_device_number_info.sparql",
    "additionalProperty": {
      "language": "de-DE"
    }
  },
  "target": {
    "@type": "DataCatalog",
    "identifier": "IQS",
    "url": "${POOLPARTY_URL}",
    "encodingFormat": "application/rdf+xml",
    "additionalProperty": {
      "username": "${POOLPARTY_USERNAME}",
      "password": "${POOLPARTY_PASSWORD}"
    }
  }
}
