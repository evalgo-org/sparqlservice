PREFIX dcterms:<http://purl.org/dc/terms/>
PREFIX skos:<http://www.w3.org/2004/02/skos/core#>
PREFIX czo:<https://schema.zeiss.com/CZ-Ontology/>
PREFIX stats:<https://schema.pantopix.com/stats#>

CONSTRUCT {
stats:allUsers stats:count ?count_nodes .
}

WHERE {
{SELECT DISTINCT (COUNT(?useruri) AS ?count_nodes)
        WHERE {
            ?useruri a czo:User .
        }}
}
