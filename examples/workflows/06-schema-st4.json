{
  "@context": "https://schema.org",
  "@type": "SearchAction",
  "identifier": "iqs-schema-st4",
  "name": "IQS: Query Schema ST4",
  "description": "Query ST4 schema data from PoolParty IQS project for a specific concept scheme",
  "query": {
    "@type": "SearchAction",
    "contentUrl": "get_schema_st4.sparql",
    "additionalProperty": {
      "concept_scheme": "https://data.zeiss.com/IQS/172"
    }
  },
  "target": {
    "@type": "DataCatalog",
    "identifier": "IQS",
    "url": "${POOLPARTY_URL}",
    "encodingFormat": "application/rdf+xml",
    "additionalProperty": {
      "username": "${POOLPARTY_USERNAME}",
      "password": "${POOLPARTY_PASSWORD}"
    }
  }
}
